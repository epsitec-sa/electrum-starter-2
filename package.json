{
  "name": "electrum-starter-2",
  "version": "0.0.1",
  "description": "Electrum starter for Electrum v2.",
  "main": "lib/index.js",
  "scripts": {
    "compile": "rimraf ./lib && npm run regen && babel -d lib src",
    "prepublish": "npm run compile",
    "regen": "electrum-require-components --wrap --crlf ./src components .component.js all.js",
    "start": "node webpack.devserver.js",
    "test": "mocha src/test/**/*.js",
    "watch": "webpack --watch --progress --colors -d",
    "webpack": "webpack -d"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epsitec-sa/electrum-starter-2"
  },
  "author": "Epsitec SA",
  "license": "MIT",
  "homepage": "https://github.com/epsitec-sa/electrum-starter-2",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators": "^6.4.0",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.2.4",
    "babel-preset-react": "^6.2.4",
    "babel-preset-stage-0": "^6.2.4",
    "chai": "^3.4.1",
    "electrum": "^2.7.1",
    "electrum-arc": "^1.9.13",
    "electrum-require-components": "^1.3.0",
    "jsdom": "^7.2.2",
    "mai-chai": "^1.1.2",
    "mocha": "^2.3.4",
    "path": "^0.12.7",
    "radium": "^0.16.2",
    "react": "^0.14.6",
    "react-addons-shallow-compare": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-element-to-jsx-string": "^2.2.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
